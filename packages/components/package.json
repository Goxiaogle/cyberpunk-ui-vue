{
    "name": "@cyberpunk-vue/components",
    "version": "1.7.2",
    "description": "Cyberpunk Vue components - A futuristic Vue 3 component library",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./*": "./*"
    },
    "files": [
        "dist",
        "src"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "vite build"
    },
    "peerDependencies": {
        "vue": "^3.5.0"
    },
    "dependencies": {
        "@cyberpunk-vue/hooks": "workspace:*",
        "@cyberpunk-vue/constants": "workspace:*",
        "@cyberpunk-vue/theme-chalk": "workspace:*"
    },
    "author": "Juxest",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/user/cyberpunk-vue.git",
        "directory": "packages/components"
    },
    "homepage": "https://github.com/user/cyberpunk-vue#readme",
    "bugs": {
        "url": "https://github.com/user/cyberpunk-vue/issues"
    },
    "keywords": [
        "vue",
        "vue3",
        "components",
        "ui",
        "cyberpunk",
        "futuristic",
        "neon",
        "design-system"
    ],
    "publishConfig": {
        "access": "public"
    }
}
