{
    "name": "cyberpunk-ui-vue",
    "version": "1.0.5",
    "description": "A futuristic Vue 3 component library with cyberpunk aesthetics",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "web-types": "./web-types.json",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./global": {
            "types": "./global.d.ts",
            "default": "./global.mjs"
        },
        "./resolver": {
            "types": "./resolver.d.ts",
            "import": "./resolver.mjs",
            "require": "./resolver.cjs",
            "default": "./resolver.mjs"
        },
        "./web-types.json": "./web-types.json",
        "./dist/*": "./dist/*",
        "./styles/*": "./styles/*"
    },
    "files": [
        "dist",
        "styles",
        "global.d.ts",
        "global.mjs",
        "resolver.mjs",
        "resolver.cjs",
        "resolver.d.ts",
        "web-types.json"
    ],
    "scripts": {
        "build": "npm run build:meta && vite build && npm run build:types && npm run build:styles",
        "build:meta": "node scripts/generate-meta.mjs",
        "build:types": "node scripts/build-types.mjs",
        "build:styles": "sass ../theme-chalk/src/index.scss styles/index.css --style=compressed"
    },
    "peerDependencies": {
        "vue": "^3.5.0"
    },
    "dependencies": {
        "@cyberpunk-vue/components": "workspace:*",
        "@cyberpunk-vue/hooks": "workspace:*",
        "@cyberpunk-vue/constants": "workspace:*",
        "@cyberpunk-vue/theme-chalk": "workspace:*"
    },
    "author": "Juxest",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Goxiaogle/cyberpunk-ui-vue.git",
        "directory": "packages/cyberpunk-vue"
    },
    "homepage": "https://github.com/Goxiaogle/cyberpunk-ui-vue#readme",
    "bugs": {
        "url": "https://github.com/Goxiaogle/cyberpunk-ui-vue/issues"
    },
    "keywords": [
        "vue",
        "vue3",
        "components",
        "ui",
        "design-system",
        "cyberpunk",
        "futuristic",
        "neon"
    ],
    "publishConfig": {
        "access": "public"
    }
}
