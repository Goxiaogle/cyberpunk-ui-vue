{
    "name": "cyberpunk-ui-vue",
    "version": "0.1.4",
    "description": "A futuristic Vue 3 component library with cyberpunk aesthetics",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./dist/*": "./dist/*",
        "./styles/*": "./styles/*"
    },
    "files": [
        "dist",
        "styles"
    ],
    "scripts": {
        "build": "vite build && npm run build:styles && npm run postbuild",
        "build:styles": "sass ../theme-chalk/src/index.scss styles/index.css --style=compressed",
        "postbuild": "echo export * from '@cyberpunk-vue/components';export * from '@cyberpunk-vue/hooks';export * from '@cyberpunk-vue/constants'; > dist/index.d.ts"
    },
    "peerDependencies": {
        "vue": "^3.5.0"
    },
    "dependencies": {
        "@cyberpunk-vue/components": "^0.1.0",
        "@cyberpunk-vue/hooks": "^0.1.0",
        "@cyberpunk-vue/constants": "^0.1.0",
        "@cyberpunk-vue/theme-chalk": "^0.1.0"
    },
    "author": "Juxest",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/user/cyberpunk-vue.git",
        "directory": "packages/cyberpunk-vue"
    },
    "homepage": "https://github.com/user/cyberpunk-vue#readme",
    "bugs": {
        "url": "https://github.com/user/cyberpunk-vue/issues"
    },
    "keywords": [
        "vue",
        "vue3",
        "components",
        "ui",
        "design-system",
        "cyberpunk",
        "futuristic",
        "neon"
    ],
    "publishConfig": {
        "access": "public"
    }
}
