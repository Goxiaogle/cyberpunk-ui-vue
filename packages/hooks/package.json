{
    "name": "@cyberpunk-vue/hooks",
    "version": "0.1.1",
    "description": "Cyberpunk Vue composables - Reusable Vue 3 composition functions",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./*": "./*"
    },
    "files": [
        "dist",
        "src"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "vite build"
    },
    "peerDependencies": {
        "vue": "^3.5.0"
    },
    "dependencies": {
        "@cyberpunk-vue/constants": "workspace:*"
    },
    "author": "Juxest",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/user/cyberpunk-vue.git",
        "directory": "packages/hooks"
    },
    "homepage": "https://github.com/user/cyberpunk-vue#readme",
    "bugs": {
        "url": "https://github.com/user/cyberpunk-vue/issues"
    },
    "keywords": [
        "vue",
        "vue3",
        "composables",
        "hooks",
        "cyberpunk"
    ],
    "publishConfig": {
        "access": "public"
    }
}
